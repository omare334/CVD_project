	keywords = {Deep learning, Machine learning, Arrhythmia detection and identification, Artificial Neural Networks, Condition monitoring, Convolutional neural networks, Fault detection, Structural damage detection, Structural health monitoring},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\omare\\Zotero\\storage\\LN4Y2WBU\\Kiranyaz et al. - 2021 - 1D convolutional neural networks and applications.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\omare\\Zotero\\storage\\39AGYV5T\\S0888327020307846.html:text/html},
