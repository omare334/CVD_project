	number = {7},
	journaltitle = {Biomedicines},
	shortjournal = {Biomedicines},
	author = {Vitolo, Marco and Proietti, Marco and Shantsila, Alena and Boriani, Giuseppe and Lip, Gregory Y. H.},
	urldate = {2023-08-27},
