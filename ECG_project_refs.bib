	keywords = {atrial fibrillation, arrhythmia detection, {CNN}, focal loss, {LSTM}},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\omare\\Zotero\\storage\\PUELM49M\\Petmezas et al. - 2021 - Automated Atrial Fibrillation Detection using a Hy.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\omare\\Zotero\\storage\\YB8KAPFR\\S1746809420303323.html:text/html},
